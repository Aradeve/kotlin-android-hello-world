{
  "name": "Android",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "customizations": {
    "vscode": {
      "settings": {
        "java.home": "/usr/lib/jvm/java-11-openjdk-amd64"
      },
      "extensions": [
        "vscjava.vscode-java-pack"
      ]
    }
  },
  "forwardPorts": [8080],
  "postCreateCommand": "yes | sdkmanager --licenses && sdkmanager --update",
  "remoteUser": "vscode"
}
